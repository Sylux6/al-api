{
    "name": "al-api",
    "version": "1.0.0",
    "description": "Api that returns azur lane data",
    "main": "src/server.ts",
    "engines": {
        "node": ">=10.x",
        "yarn": ">=1.15.2"
    },
    "homepage": "https://github.com/KurozeroPB/al-api#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/KurozeroPB/al-api.git"
    },
    "bugs": {
        "email": "bugs@kurozeropb.info",
        "url": "https://github.com/KurozeroPB/al-api/issues"
    },
    "author": {
        "email": "root@kurozeropb.info",
        "name": "KurozeroPB",
        "url": "https://kurozeropb.info"
    },
    "license": "MIT",
    "private": false,
    "scripts": {
        "start": "yarn serve",
        "serve": "cross-env NODE_ENV=production node build/src/server.js",
        "dev": "cross-env NODE_ENV=development node build/src/server.js",
        "test": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/server.ts",
        "pretest": "yarn check",
        "postinstall": "yarn compile",
        "copy-assets": "ts-node src/tools/copyAssets",
        "compile": "yarn clean && tsc -p . && yarn copy-assets",
        "check": "gts check",
        "clean": "gts clean",
        "fix": "gts fix",
        "docs:dev": "vuepress dev docs",
        "docs:build": "vuepress build docs",
        "docs:publish": "./deploy.sh"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "chalk": "^2.4.2",
        "cheerio": "^1.0.0-rc.3",
        "cross-env": "^6.0.3",
        "ejs": "^2.7.1",
        "express": "^4.17.1",
        "express-rate-limit": "^5.0.0",
        "moment": "^2.24.0",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/cheerio": "^0.22.13",
        "@types/express": "^4.17.1",
        "@types/express-rate-limit": "^3.3.3",
        "@types/node": "^12.11.7",
        "gts": "^1.1.0",
        "ts-node": "^8.4.1",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.6.4",
        "vuepress": "^1.2.0"
    }
}
